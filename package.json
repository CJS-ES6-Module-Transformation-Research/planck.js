{
    "name": "planck-js",
    "version": "0.2.7",
    "description": "2D JavaScript physics engine for cross-platform HTML5 game development",
    "homepage": "http://shakiba.me/planck.js",
    "keywords": [
        "box2d",
        "html5",
        "javascript",
        "game",
        "physics",
        "engine",
        "2d",
        "mobile"
    ],
    "license": "zlib",
    "repository": {
        "type": "git",
        "url": "git://github.com/shakiba/planck.js.git"
    },
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "dist/",
        "doc/",
        "lib/",
        "test/",
        "testbed/"
    ],
    "author": "Ali Shakiba",
    "contributors": [
        {
            "name": "Ali Shakiba",
            "url": "http://shakiba.me/planck.js"
        },
        {
            "name": "Erin Catto",
            "url": "http://box2d.org/"
        }
    ],
    "devDependencies": {
        "browserify": "^8.1.3",
        "browserify-middleware": "^7.0.0",
        "dtslint": "^0.3.0",
        "expect.js": "*",
        "express": "^4.13.4",
        "gulp": "*",
        "gulp-header": "^1.7.1",
        "gulp-rename": "^1.2.0",
        "gulp-uglify": "*",
        "gulp-util": "^3.0.5",
        "handlebars": "^4.0.6",
        "nodemon": "^1.11.0",
        "serve-index": "^1.7.3",
        "sinon": "*",
        "stage-js": "^0.8.10",
        "tslint": "^5.11.0",
        "typescript": "^3.0.3",
        "uglifyify": "^3.0.1",
        "vinyl-buffer": "^1.0.0",
        "vinyl-source-stream": "^1.0.0",
        "vinyl-transform": "^1.0.0",
        "babel-cli": "latest",
        "babel-core": "latest",
        "babel-plugin-add-module-exports": "latest",
        "babel-plugin-transform-es2015-modules-commonjs": "latest",
        "babel-polyfill": "latest",
        "babel-plugin-transform-object-rest-spread": "latest",
        "babel-preset-env": "latest"
    },
    "scripts": {
        "test": "mocha test/*.js",
        "dist": "gulp",
        "testbed": "nodemon testbed/server.js",
        "compile-lib": "babel lib --out-dir lib --require babel-polyfill --presets env",
        "compile-libshape": "babel lib\\shape --out-dir lib\\shape --require babel-polyfill --presets env",
        "compile-test": "babel test --out-dir test --require babel-polyfill --presets env",
        "compile-libcommon": "babel lib\\common --out-dir lib\\common --require babel-polyfill --presets env",
        "compile-libjoint": "babel lib\\joint --out-dir lib\\joint --require babel-polyfill --presets env",
        "compile-testbed": "babel testbed --out-dir testbed --require babel-polyfill --presets env",
        "compile-libutil": "babel lib\\util --out-dir lib\\util --require babel-polyfill --presets env",
        "compile-libcollision": "babel lib\\collision --out-dir lib\\collision --require babel-polyfill --presets env",
        "compile-testtestutil": "babel test\\testutil --out-dir test\\testutil --require babel-polyfill --presets env",
        "compile-1_script": "npm run compile-lib && npm run compile-libshape && npm run compile-test && npm run compile-libcommon && npm run compile-libjoint && npm run compile-testbed && npm run compile-libutil && npm run compile-libcollision && npm run compile-testtestutil",
        "compile": "npm run compile-1_script"
    }
}